<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:gravity="center|center_vertical"
        android:orientation="vertical"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        tools:ignore="MissingDefaultResource">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/cimg_user"
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"
            android:background="@color/white"
            android:src="@mipmap/dummy_avatar"
            app:civ_border_color="@color/colorPrimary"
            app:civ_border_width="1dp" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_toLeftOf="@+id/iv_arrow_nav"
            android:layout_toRightOf="@+id/cimg_user"
            android:gravity="center_vertical"
            android:orientation="vertical">

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txt_username"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:fontFamily="@font/faktpro_semibold"
                android:lineSpacingExtra="6sp"
                android:text="@string/str_user_name"
                android:textColor="@color/textColorBlack"
                android:textSize="15sp" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txt_team_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:fontFamily="@font/faktpro_normal"
                android:lineSpacingExtra="6sp"
                android:text="@string/str_mobile_number"
                android:textColor="@color/textColorLightBlack"
                android:textSize="15sp" />

        </LinearLayout>

        <ImageView
            android:id="@+id/iv_arrow_nav"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/profile_arrow" />


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rl_import_fb"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/colorASPrimary"
        android:visibility="gone">


        <LinearLayout
            android:layout_width="217dp"
            android:layout_height="29dp"
            android:layout_centerVertical="true"
            android:layout_margin="17dp"
            android:background="@drawable/bgfacebook">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="11.9dp"
                android:src="@mipmap/facebook" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="8.2dp"
                android:text="@string/str_import_facebook"
                android:textColor="@color/textColorBlack"
                android:textSize="12sp" />

        </LinearLayout>


    </RelativeLayout>

</LinearLayout>


